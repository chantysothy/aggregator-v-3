!!!
%html
  %head
    %title Aggregator
    %meta{ name: 'viewport', content: 'width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' }
    %meta{ name: 'apple-mobile-web-app-capable', content: 'yes' }
    %meta{ name: 'apple-mobile-web-app-status-bar-style', content: 'white' }
    %meta{ name: 'format-detection', content: 'telephone=yes' }
    %meta{ name: 'mobile-web-app-capable', content: 'yes' }
    %script{ src: 'https://use.typekit.net/kkm1ywq.js' }
    :javascript
      try{Typekit.load();}catch(e){}
    = stylesheet_link_tag 'application'
    = csrf_meta_tags
  %body
    = render partial: 'shared/navbar'#, locals: { current_user: current_user }
    - unless flash.nil? || flash.empty?
      = render partial: 'shared/flash', locals: { flash: flash }

    %main
      = yield

    %script{ src:'https://code.jquery.com/jquery-2.2.3.js', integrity: 'sha256-laXWtGydpwqJ8JA+X9x2miwmaiKhn8tVmOVEigRNtP4=', crossorigin: "anonymous" }
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
